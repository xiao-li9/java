package week4;

public class Rectangle {

	float length;
	float width;
	public Rectangle() {
		length=0.1f;
		width=0.1f;
	}
	public Rectangle(float length,float width) {
		this.length=length;
		this.width=width;
	}
	public void Calculate() {
		float area;
		float perimeter;
		area=length*width;
		perimeter=(length+width)*2;
		System.out.println("area:"+area);
		System.out.println("perimeter:"+perimeter);
	}
}
